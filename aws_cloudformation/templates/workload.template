        "StackCreateFortiGateNatGateway": {
            "Type": "AWS::CloudFormation::Stack",
            "Properties": {
                "Parameters": {
                    "QSS3BucketName": {
                        "Ref": "QSS3BucketName"
                    },
                    "QSS3KeyPrefix": {
                        "Ref": "QSS3KeyPrefix"
                    },
                    "CustomIdentifier": {
                        "Ref": "CustomIdentifier"
                    },
                    "VpcId": {
                        "Ref": "VpcId"
                    },
                    "SubnetId1": {
                        "Ref": "PublicSubnet1"
                    },
                    "SubnetId2": {
                        "Ref": "PrivateSubnet1"
                    },
                    "SubnetId3": {
                        "Ref": "PublicSubnet2"
                    },
                    "SubnetId4": {
                        "Ref": "PrivateSubnet2"
                    },
                    "FortiGateImageId": {
                        "Fn::FindInMap": [
                            "AWSAMIRegionMap",
                            {
                                "Ref": "AWS::Region"
                            },
                            {
                                "Fn::FindInMap": [
                                    "FortiGateKeyMap",
